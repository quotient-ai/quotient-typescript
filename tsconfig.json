{
  "compilerOptions": {
    "target": "es2020",
    "module": "commonjs",
    "declaration": true,
    "outDir": "./dist",
    "strict": true,
    "esModuleInterop": true,
    "skipLibCheck": true,
    "forceConsistentCasingInFileNames": true,
    "moduleResolution": "node",
    "resolveJsonModule": true,
    "baseUrl": ".",
    "paths": {
      "*": ["node_modules/*"]
    }
  },
  "include": ["quotientai/src/**/*", "quotientai/resources", "quotientai/client.ts", "quotientai/errors.ts", "quotientai/index.ts", "quotientai/logger.ts", "quotientai/types.ts"],
  "exclude": ["node_modules", "dist"]
} 